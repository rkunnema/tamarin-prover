lemma recentalive_tag:
  exists-trace
  "¬(∀ x y #i.
      (Alive( x, y ) @ #i) ⇒
      (∃ #j #k.
        (((Challenge( x ) @ #k) ∧ (Response( y ) @ #j)) ∧ (#k < #j)) ∧
        (#j < #i)))"
/*
guarded formula characterizing all satisfying traces:
"∃ x y #i.
  (Alive( x, y ) @ #i)
 ∧
  ∀ #j #k.
   (Challenge( x ) @ #k) ∧ (Response( y ) @ #j)
  ⇒
   ((¬(#k < #j)) ∨ (¬(#j < #i)))"
*/
simplify
solve( State_111111111( ~k, ~na, m, nb ) ▶₀ #i )
  case ifhknanbm_0_11111111
  solve( splitEqs(1) )
    case split_case_7
    solve( !KU( h((~k⊕x)) ) @ #vk.1 )
      case outhknbnanb_0_112111211
      solve( !KU( (~na⊕x) ) @ #vk.2 )
        case c_xor
        solve( splitEqs(3) )
          case split_case_2
          solve( !KU( (~nb⊕na.1) ) @ #vk.4 )
            case c_xor
            solve( !KU( ~na ) @ #vk.5 )
              case outna_0_111111
              solve( !KU( ~nb ) @ #vk.6 )
                case outhknbnanb_0_112111211
                SOLVED // trace found
              qed
            qed
          qed
        qed
      qed
    qed
  qed
qed

lemma executable:
  exists-trace
  "∃ x y #i #j.
    (((Alive( x, y ) @ #i) ∧ (Response( y ) @ #j)) ∧
     (¬(∃ #k. (Response( y ) @ #k) ∧ (¬(#j = #k))))) ∧
    (¬(∃ #l. NeqForExec( x, x ) @ #l))"
/*
guarded formula characterizing all satisfying traces:
"∃ x y #i #j.
  (Alive( x, y ) @ #i) ∧ (Response( y ) @ #j)
 ∧
  (∀ #k. (Response( y ) @ #k) ⇒ #j = #k) ∧
  (∀ #l. (NeqForExec( x, x ) @ #l) ⇒ ⊥)"
*/
simplify
solve( State_111111111( ~k, ~na, m, nb ) ▶₀ #i )
  case ifhknanbm_0_11111111
  solve( State_11211121( ~k, ~nb.1, na.1 ) ▶₀ #j )
    case eventNeqForExecnazero_0_1121112
    solve( splitEqs(2) )
      case split_case_3
      solve( !KU( h((~k⊕~na⊕nb)) ) @ #vk.1 )
        case outhknbnanb_0_112111211
        solve( splitEqs(4) )
          case split_case_2
          solve( !KU( ~nb ) @ #vk.2 )
            case outhknbnanb_0_112111211
            solve( !KU( ~na ) @ #vk.3 )
              case outna_0_111111
              SOLVED // trace found
            qed
          qed
        qed
      qed
    qed
  qed
qed











